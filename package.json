{
    "name": "@volverjs/zod-vue-i18n",
    "type": "module",
    "version": "0.0.0",
    "packageManager": "pnpm@9.15.4",
    "description": "Translating zod error messages with vue-i18n.",
    "author": "8 Wave",
    "license": "MIT",
    "homepage": "https://github.com/volverjs/zod-vue-i18n",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/zod-vue-i18n"
    },
    "bugs": {
        "url": "https://github.com/volverjs/zod-vue-i18n/issues"
    },
    "keywords": [
        "zod",
        "vue3",
        "i18n",
        "vue-i18n"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./src/*": "./src/*",
        "./locales/*": "./locales/*",
        "./dist/*": "./dist/*"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "files": [
        "*.d.ts",
        "dist",
        "locales",
        "src"
    ],
    "engines": {
        "node": ">= 16.x"
    },
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit",
        "build": "tsc && copy src/**/*.d.ts dist",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
        "credits": "npx @opengovsg/credits-generator"
    },
    "peerDependencies": {
        "vue": "^3.5.*",
        "vue-i18n": "^11.*",
        "zod": "^3.*"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^4.17.0",
        "@types/jest": "^30.0.0",
        "@types/qs": "^6.9.18",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "copy": "^0.3.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "jest": "^30.0.4",
        "jest-fetch-mock": "^3.0.3",
        "jest-localstorage-mock": "^2.4.26",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.3"
    }
}
