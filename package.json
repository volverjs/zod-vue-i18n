{
    "name": "@volverjs/zod-vue-i18n",
    "type": "module",
    "version": "0.0.0",
    "packageManager": "pnpm@10.23.0",
    "description": "Translating zod error messages with vue-i18n.",
    "author": "8 Wave",
    "license": "MIT",
    "homepage": "https://github.com/volverjs/zod-vue-i18n",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/zod-vue-i18n"
    },
    "bugs": {
        "url": "https://github.com/volverjs/zod-vue-i18n/issues"
    },
    "keywords": [
        "zod",
        "vue3",
        "i18n",
        "vue-i18n"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        },
        "./v4": {
            "types": "./dist/v4/index.d.mts",
            "import": "./dist/v4/index.mjs"
        },
        "./locales/*": "./locales/*"
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.mts"
            ]
        }
    },
    "files": [
        "*.d.ts",
        "dist",
        "locales"
    ],
    "engines": {
        "node": ">= 16.x"
    },
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit",
        "build": "tsdown",
        "test": "vitest run",
        "test:watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "peerDependencies": {
        "vue-i18n": "^11.2.0",
        "zod": "^3.25.0 || ^4.0.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^6.2.0",
        "eslint": "^9.39.1",
        "tsdown": "^0.17.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.14",
        "vue": "^3.5.25",
        "vue-i18n": "^11.2.2",
        "zod": "^4.1.13"
    }
}
